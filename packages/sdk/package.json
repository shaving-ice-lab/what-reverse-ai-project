{
  "name": "@agentflow/sdk",
  "version": "0.1.0",
  "description": "AgentFlow 自定义节点 SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "agentflow": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts",
    "build:cli": "tsup src/cli/index.ts --format cjs --dts",
    "dev": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agentflow",
    "sdk",
    "workflow",
    "node",
    "plugin",
    "cli"
  ],
  "author": "AgentFlow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentflow/agentflow.git",
    "directory": "packages/sdk"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
